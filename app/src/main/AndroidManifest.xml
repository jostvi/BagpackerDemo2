<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="se.mau.ai9856.bagpackerdemo">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".TripDate"></activity>
        <activity android:name=".TripActivity" />
        <activity android:name=".Transport" />
        <activity android:name=".Destination" />
        <activity android:name=".DatePicker" />
        <activity android:name=".ChoosePackinglistName" />
        <activity android:name=".Accomodation" />
        <activity android:name=".Login" />
        <activity android:name=".ShowSavedListActivity" />
        <activity android:name=".CreateTripActivity" />
        <activity android:name=".EditableListActivity" />
        <activity android:name=".CreateNewAccount" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>